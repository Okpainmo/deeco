{
  "extends": ["plugin:prettier/recommended", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    // "prettier/prettier": "error",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "endOfLine": "auto"
      }
    ],
    "no-console": "off",
    "no-alert": "off",
    "no-unused-vars": "off",
    "camelcase": "off",
    "no-param-reassign": "off",
    "import/prefer-default-export": "off"
  }
}
